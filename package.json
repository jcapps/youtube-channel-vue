{
  "name": "youtube-channel-vue",
  "version": "1.0.0",
  "description": "Site showcasing my YouTube channel",
  "scripts": {
    "start": "npm-run-all --parallel open:src lint:watch test:watch",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tests tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha-webpack --webpack-config ./webpack.config.test.js --require tools/testSetup.js \"tests/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist lint build:html",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js"
  },
  "author": "James Capps",
  "license": "",
  "devDependencies": {
    "cheerio": "1.0.0-rc.2",
    "colors": "1.1.2",
    "compression": "1.7.0",
    "css-loader": "0.28.4",
    "eslint": "4.4.1",
    "eslint-plugin-html": "4.0.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-vue": "2.1.0",
    "eslint-watch": "3.1.2",
    "eventsource-polyfill": "0.9.6",
    "expect": "1.20.2",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "html-loader": "0.5.1",
    "jsdom": "11.1.0",
    "mocha": "3.5.0",
    "mocha-webpack": "1.0.1",
    "node-sass": "4.5.3",
    "npm-check-updates": "2.12.1",
    "npm-run-all": "4.0.2",
    "open": "0.0.5",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.6",
    "url-loader": "0.5.9",
    "vue-loader": "13.5.0",
    "vue-style-loader": "3.0.3",
    "vue-template-compiler": "2.5.9",
    "vue-test-utils": "1.0.0-beta.6",
    "vuegister": "0.2.2",
    "webpack": "3.5.5",
    "webpack-dev-middleware": "1.12.0",
    "webpack-hot-middleware": "2.18.2"
  },
  "dependencies": {
    "axios": "0.16.2",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-vue": "1.2.1",
    "babel-register": "6.26.0",
    "body-parser": "1.17.2",
    "bootstrap": "3.3.7",
    "dompurify": "0.9.0",
    "express": "4.15.4",
    "jquery": "3.2.1",
    "moxios": "0.4.0",
    "sinon": "3.2.0",
    "toastr": "2.1.2",
    "vue": "2.5.9",
    "vue-router": "3.0.1",
    "vuex": "3.0.1",
    "vuex-router-sync": "5.0.0",
    "youtube-player": "5.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jcapps/youtube-channel-vue.git"
  }
}
